		    <? echo $Array_Total_resposta_Regular[$Regional]; //REGULAR?><BR></FONT>
		</td>
		<td style="border-top: 1px solid #3c3c3c; border-bottom: 1px solid #3c3c3c; border-left: 1px solid #3c3c3c; border-right: 1px solid #3c3c3c" ALIGN="CENTER"><FONT COLOR="#000000">
		     <? echo $Array_Total_resposta_Ruim[$Regional]; //RUIM?><BR></FONT>
		</td>
		<td style="border-top: 1px solid #3c3c3c; border-bottom: 1px solid #3c3c3c; border-left: 1px solid #3c3c3c; border-right: 1px solid #3c3c3c" ALIGN="CENTER"><FONT COLOR="#000000">
