	<? 
	$sqlPesquisa = "SELECT regional, pergunta1, count(*) as total FROM `girpesquisaresposta` WHERE Semana LIKE $Semana_Escolhida AND Ano LIKE $Ano_Escolhida GROUP BY regional, pergunta1 ORDER BY regional";
	$ResPesquisa = mysql_query($sqlPesquisa) or die("Falha no comando sqlPesquisa");
	$TotregPesquisa = mysql_num_rows($ResPesquisa);//qtde de tecnicos que registraram execução, aqui contam o numero de execucoes independe se for mesmo tec
	
	$Array_Total_respostaM = Array();
	$Array_Total_respostaF = Array();
	foreach ($Array_Regionais as $Regional => $NomeDaRegional) { //preenche com valor zero para todas as regionais em ambos os vetores, isso antes de contar
	  $Array_Total_respostaM[$Regional] = 0;
	  $Array_Total_respostaF[$Regional] = 0;
	}
	while($consulta = mysql_fetch_array($ResPesquisa)) {
	  $Regional = $consulta['regional'];	  
	  if ( $consulta['pergunta1'] == "FEMININO" ) {
	    $Array_Total_respostaF[$Regional] = $consulta['total'];	    
	  } else {
	    $Array_Total_respostaM[$Regional] = $consulta['total'];
	  }
	}	
	foreach ($Array_Total_respostaF as $Regional => $TotalResposta) {
	?>
