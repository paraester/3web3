	<? 
	$sqlPesquisa = "SELECT regional, pergunta4, count(*) as total FROM `girpesquisaresposta` GROUP BY regional, pergunta4 ORDER BY regional";
	$ResPesquisa = mysql_query($sqlPesquisa) or die("Falha no comando sqlPesquisa");
	$TotregPesquisa = mysql_num_rows($ResPesquisa);//qtde de tecnicos que registraram execução, aqui contam o numero de execucoes independe se for mesmo tec
	
	$Array_Total_resposta_Otimo = Array();
	$Array_Total_resposta_Bom = Array();
	$Array_Total_resposta_Regular = Array();
	$Array_Total_resposta_Ruim = Array();
	$Array_Total_resposta_Pessimo = Array();
	$Array_Total_resposta_Nao = Array();
	
	foreach ($Array_Regionais as $Regional => $NomeDaRegional) { //preenche com valor zero para todas as regionais em ambos os vetores, isso antes de contar
	  $Array_Total_resposta_Otimo[$Regional] = 0;
	  $Array_Total_resposta_Bom[$Regional] = 0;
	  $Array_Total_resposta_Regular[$Regional] = 0;
	  $Array_Total_resposta_Ruim[$Regional] = 0;
	  $Array_Total_resposta_Pessimo[$Regional] = 0;
	  $Array_Total_resposta_Nao[$Regional] = 0;
	}
	while($consulta = mysql_fetch_array($ResPesquisa)) {
	  $Regional = $consulta['regional'];	  

	  $i = $consulta['pergunta4'];
	  switch ($i) {
	      case "a": //ótimo
		  $Array_Total_resposta_Otimo[$Regional] = $consulta['total'];
		  break;
	      case "b": //bom
		  $Array_Total_resposta_Bom[$Regional] = $consulta['total'];
		  break;
	      case "c": //Regular
		  $Array_Total_resposta_Regular[$Regional] = $consulta['total'];
		  break;
	      case "d": //ruim
		  $Array_Total_resposta_Ruim[$Regional] = $consulta['total'];
		  break;
	      case "e": //pessimo
		  $Array_Total_resposta_Pessimo[$Regional] = $consulta['total'];
		  break;
	      case "f": //nao
		  $Array_Total_resposta_Nao[$Regional] = $consulta['total'];
		  break;
	  }

	} //fim do while

